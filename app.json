{
	"expo": {
		"name": "Pokeclick",
		"slug": "Pokeclick",
		"version": "1.0.0",
		"scheme": "pokeclick",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/miaoussLoader.gif",
			"resizeMode": "contain",
			"backgroundColor": "#feffe9"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/miaoussLoader.gif",
				"backgroundColor": "#feffe9"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"]
	}
}
